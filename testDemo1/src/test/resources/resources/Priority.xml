<parameter name="sleepTime" value="3000"/>
	<test name="Parameters_usingLogin">
	<parameter name="browserName" value="Firefox"/>
		<classes>
			<class name="testDemo1.Parameters_using">
				<methods>
					<include name="LaunchApp" />
